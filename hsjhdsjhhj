local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))()
local Window =
Rayfield:CreateWindow(
    {
        Name = "Blox Fruits",
        LoadingTitle = "Blox Fruits [EVENT!]",
        LoadingSubtitle = "Script Loading..",
        ConfigurationSaving = {
            Enabled = true,
            FolderName = nil,
            FileName = "BloxFruits(Event)"
        },
        Discord = {
            Enabled = false,
            Invite = "",
            RememberJoins = true
        },
        KeySystem = true,
        KeySettings = {
            Title = "Event Hub",
            Subtitle = "Key System",
            Note = "Join the discord",
            FileName = "BloxFruitsKey",
            SaveKey = true,
            GrabKeyFromSite = false,
            Key = "best"
        }
    }
)

-- CREDITS TAB

local CreditsTab = Window:CreateTab("Credits")
